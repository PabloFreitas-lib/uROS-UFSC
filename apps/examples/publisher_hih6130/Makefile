-include $(TOPDIR)/Make.defs

# micro-ROS-publisher built-in application info

CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_HIH6130_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_HIH6130_STACKSIZE ?= 65000

APPNAME = publisher_hih6130
PRIORITY = $(CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_HIH6130_PRIORITY)
STACKSIZE = $(CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_HIH6130_STACKSIZE)

# uROS-publisher Example

ASRCS =
CSRCS =
MAINSRC = publisher_hih6130_main.c 

CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_PROGNAME ?= publisher_hih6130$(EXEEXT)
PROGNAME = $(CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_HIH6130_PROGNAME)
UROSDIR = $(shell find $(APPDIR)/../uros_ws/install -type d -name include)
CFLAGS += ${shell $(INCDIR) $(INCDIROPT) "$(CC)" "$(UROSDIR)"} -std=c99

MODULE = CONFIG_MICRO_ROS_EXAMPLES_PUBLISHER_HIH6130

include $(APPDIR)/Application.mk
